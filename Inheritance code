class Student:
  def __init__(self, fname, lname):
    self.firstname = fname
    self.lastname = lname

  def printname(self):
    print(self.firstname, self.lastname)

#Use the Student class to create an object, and then execute the printname method:

x = Student("Sachin", "Kholi")
x.printname()

#@Output : ('Sachin', 'Kholi')
